{
  "version": 2,
  "builds": [
    {
      "src": "django_shop/core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/django_shop/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "django_shop/core/wsgi.py"
    }
  ]
}

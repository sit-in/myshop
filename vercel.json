{
  "version": 2,
  "buildCommand": "bash build_files.sh",
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-report/",
      "schedule": "0 14 * * *"
    }
  ]
}
